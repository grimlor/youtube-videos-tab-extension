{
  "name": "youtube-videos-tab-extension",
  "version": "1.0.0",
  "type": "module",
  "description": "Chromium extension that automatically navigates to the Videos tab on YouTube channel pages",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "youtube",
    "chrome-extension",
    "chromium",
    "browser-extension"
  ],
  "author": "Jack Pines (grimlor)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/chrome": "^0.0.278",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
